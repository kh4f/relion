{{#*inline "compareLink"~}}
	{{repo.homepage~}}
	{{#if prevRelease.tag~}}
		/compare/{{prevRelease.tag}}...
	{{~else~}}
		/commits/
	{{~/if}}
	{{~tag}}
{{~/inline}}

{{~#*inline "commit"~}}
	{{{subject}}} {{" "}}
	{{~#if @root.commitHyperlink~}}
		[`{{hash}}`]({{@root.repo.homepage}}/commit/{{hash}})
	{{~else~}}
		{{hash}}
	{{~/if~}}
	{{~#if breakingChanges}} ‚ö†Ô∏è<sup>[{{breakingChangeIndex}}]</sup>{{/if}}
{{~/inline}}

{{~#*inline "scope"~}}
	{{#if scope}}**{{scope}}**: {{/if}}
{{~/inline}}

{{~#>header~}}
	## &ensp; [` üì¶ {{tag}}  `]({{>compareLink}})

{{/header}}

{{~#>main~}}
	{{#each commitTypeGroups~}}
		### {{{repeat '&nbsp;' 5}}}{{title}}
		{{#if (eq @key 'breaking')}}
			{{#each commits}}
				{{#if (isArray breakingChanges)}}
					{{~#each breakingChanges~}}
						- {{{this}}} {{#if (isBreakingCommitInOtherTypeGroup ../this)}}<sup>[{{../breakingChangeIndex}}]</sup>{{/if}}
					{{/each}}
				{{else~}}
					- {{{breakingChanges}}} {{#if (isBreakingCommitInOtherTypeGroup this)}}<sup>[{{breakingChangeIndex}}]</sup>{{/if}}
				{{/if}}
			{{/each}}
		{{else}}
			{{#if scopeGroups}}
				{{#each scopeGroups~}}
					{{~#if (or (isSingle commits) (not scope))}}
						{{#each commits~}}
							- {{>scope}}{{>commit}}
						{{/each}}
					{{else~}}
						- {{>scope}}{{>br}}{{~#each commits}}  {{" "~}}
							- {{>commit}}
						{{/each}}
					{{/if}}
				{{/each}}
			{{else}}
				{{#each commits~}}
					- {{>scope}}{{>commit}}
				{{/each}}
			{{/if}}
		{{/if}}

	{{/each}}
{{/main}}

{{~#>footer~}}
	##### &emsp;&ensp;&nbsp;&nbsp; [_All Release Commits_]({{>compareLink}}) &ensp;‚Ä¢&ensp; _{{date}}_


{{/footer}}