				{{/if}}
			{{/each}}
		{{else}}
			{{#if scopeGroups}}
				{{#each scopeGroups~}}
					{{~#if (or (isSingle commits) (not scope))}}
						{{#each commits~}}
							- {{>scope}}{{>commit}}
						{{/each}}
					{{else~}}
						- {{>scope}}{{>br}}{{~#each commits}}  {{" "~}}
							- {{>commit}}
						{{/each}}
					{{/if}}
				{{/each}}
			{{else}}
				{{#each commits~}}
					- {{>scope}}{{>commit}}
				{{/each}}
			{{/if}}
		{{/if}}

	{{/each}}
{{/main}}

